Cell["\<\
Verifying size-monotonicity axiom of recombination P-structures\
\>", "Subsection",
 CellChangeTimes->{{3.781020245106422*^9, 
  3.78102032445112*^9}},ExpressionUUID->"5fef4eb9-ca87-4c72-bf81-\
a31d48341276"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Size", "-", 
     RowBox[{"monotonicity", ":", " ", 
      RowBox[{"if", " ", "z", " ", "in", " ", "R", 
       RowBox[{"(", 
        RowBox[{"x", ",", "y"}], ")"}]}]}]}], " ", "=", 
    RowBox[{">", " ", 
     RowBox[{"|", 
      RowBox[{"R", 
       RowBox[{"(", 
        RowBox[{"x", ",", "z"}], ")"}]}], "|", " ", 
      RowBox[{"\[LessEqual]", " ", 
       RowBox[{"|", 
        RowBox[{"R", 
         RowBox[{"(", 
          RowBox[{"x", ",", "y"}], ")"}]}], "|"}]}]}]}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"areMonotonic", "[", 
      RowBox[{"a1_", ",", "a2_", ",", "b1_", ",", "b2_", ",", "dim_"}], "]"}],
      ":=", 
     RowBox[{
      RowBox[{"Length", "[", 
       RowBox[{"hammingIntersect", "[", 
        RowBox[{"a1", ",", "a2", ",", "dim"}], "]"}], "]"}], "\[LessEqual]", 
      " ", 
      RowBox[{"Length", "[", 
       RowBox[{"hammingIntersect", "[", 
        RowBox[{"b1", ",", "b2", ",", "dim"}], "]"}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"areChildrenMonotonic", "[", 
      RowBox[{"x_", ",", "y_", ",", "dim_"}], "]"}], ":=", "  ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"areMonotonic", "[", 
          RowBox[{"x", ",", "#", ",", "x", ",", "y", ",", "dim"}], "]"}], 
         ")"}], "&"}], ")"}], " ", "/@", " ", 
      RowBox[{"hammingIntersect", "[", 
       RowBox[{"x", ",", "y", ",", "dim"}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"monotonicity", "[", "dim_", "]"}], ":=", 
     RowBox[{"And", " ", "@@", " ", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"areChildrenMonotonic", "[", 
          RowBox[{"i", ",", "j", ",", "dim"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"hammingSeqs", "[", "dim", "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", 
           RowBox[{"hammingSeqs", "[", "dim", "]"}]}], "}"}]}], "]"}], 
       "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Uncomment", " ", "and", " ", "evaluate", " ", "the", " ", "next", " ", 
     "line"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"monotonicity", "[", "3", "]"}]}]}]], "Input",
 CellChangeTimes->{{3.78102032891046*^9, 3.781020353848259*^9}, {
