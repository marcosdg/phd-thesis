Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Auxiliary", " ", "functions"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"hammingSeqs", "[", "dim_", "]"}], ":=", 
     RowBox[{"Tuples", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}], ",", "dim"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"isInHammingBall", "[", 
      RowBox[{"p_", ",", "c_", ",", "r_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"HammingDistance", "[", 
       RowBox[{"c", ",", "p"}], "]"}], "\[LessEqual]", "r"}]}], ";", 
    RowBox[{
     RowBox[{"hammingBall", "[", 
      RowBox[{"c_", ",", "r_", ",", "dim_"}], "]"}], ":=", 
     RowBox[{"Select", "[", 
      RowBox[{
       RowBox[{"hammingSeqs", "[", "dim", "]"}], ",", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", "isInHammingBall", ")"}], "[", 
          RowBox[{"#", ",", "c", ",", "r"}], "]"}], "&"}], ")"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"numNeighboursAtDistance", "[", 
      RowBox[{"d_", ",", "dim_"}], "]"}], ":=", 
     RowBox[{"Binomial", "[", 
      RowBox[{"dim", ",", "d"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"hammingBallSize", "[", 
      RowBox[{"r_", ",", "dim_"}], "]"}], ":=", 
     RowBox[{"Total", "[", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"numNeighboursAtDistance", "[", 
         RowBox[{"i", ",", "dim"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "0", ",", "r"}], "}"}]}], "]"}], "]"}]}], 
    ";"}]}]}]], "Input",
