Cell[TextData[StyleBox["Each binary crossover operator can be defined in \
terms of a binary 'mask', indicating for each bit position of the 'offspring' \
whether the value at that position is taken from the 'father' or the \
'mother'. The uniform crossover operator is defined by all possible masks \
applied to the parents.",
 FontFamily->"Latin Modern Roman"]], "Text",
 CellChangeTimes->{{3.7260475591527042`*^9, 3.726047560072856*^9}, {
  3.728459500352386*^9, 3.728459503391445*^9}, {3.7284595571124077`*^9, 
  3.728459638976615*^9}, {3.728459671368443*^9, 3.7284597263845654`*^9}, {
  3.728459758520524*^9, 3.728459899264945*^9}, {3.7284601266647463`*^9, 
  3.72846012669695*^9}, {3.728464095924844*^9, 3.728464095926868*^9}, {
  3.7284657921974707`*^9, 3.728465792200918*^9}, {3.7284661723475933`*^9, 
  3.728466172350266*^9}, {3.7810002743583508`*^9, 3.781000275193989*^9}, {
  3.782999541367342*^9, 
  3.7829995422796288`*^9}},ExpressionUUID->"47762b1b-195e-4f07-8bb4-\
addb626cb2dc"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"masks", "[", "dim_", "]"}], ":=", 
   RowBox[{"Tuples", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}], ",", "dim"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"crossoverMasked", "[", 
    RowBox[{"father_", ",", "mother_", ",", "mask_", ",", "dim_"}], "]"}], ":=",
    "\[IndentingNewLine]", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"mask", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "\[Equal]", "0"}], ",", 
       "\[IndentingNewLine]", "\t\t\t", 
       RowBox[{"father", "[", 
        RowBox[{"[", "i", "]"}], "]"}], ",", "\[IndentingNewLine]", "\t\t\t", 
       
       RowBox[{"mother", "[", 
        RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", "\[IndentingNewLine]", 
     "\t ", 
     RowBox[{"{", 
      RowBox[{"i", ",", "dim"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"unifCrossover", "[", 
    RowBox[{"father_", ",", "mother_", ",", "dim_"}], "]"}], ":=", " ", 
   RowBox[{"DeleteDuplicates", "[", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"crossoverMasked", "[", 
         RowBox[{"father", ",", "mother", ",", "#", ",", "dim"}], "]"}], 
        ")"}], "&"}], ")"}], "/@", 
     RowBox[{"masks", "[", "dim", "]"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.726047855037455*^9, 3.7260478550442667`*^9}, {
  3.7260482346030207`*^9, 3.726048267988949*^9}, {3.726048959442625*^9, 
