Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"fLO", ":", " ", 
    RowBox[{
     RowBox[{
     "the", " ", "fitness", " ", "function", " ", "of", " ", "leading"}], "-",
      "ones"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"fLO", "[", 
      RowBox[{"dim_", ",", "onesZeros_"}], "]"}], ":=", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{"Product", "[", 
        RowBox[{
         RowBox[{"onesZeros", "[", 
          RowBox[{"[", "j", "]"}], "]"}], ",", "\[IndentingNewLine]", 
         "                                                                    \
   ", 
         RowBox[{"{", 
          RowBox[{"j", ",", "1", ",", "i"}], "}"}]}], "]"}], ",", " ", 
       "\[IndentingNewLine]", 
       "                                                       ", 
       RowBox[{"{", 
        RowBox[{"i", ",", " ", "1", ",", " ", "dim"}], "}"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"fsLO", ":", " ", 
     RowBox[{
     "the", " ", "image", " ", "of", " ", "the", " ", "fitness", " ", 
      "function"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"fsLO", "[", "dim_", "]"}], ":=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"fLO", "[", 
         RowBox[{"dim", ",", "#"}], "]"}], "&"}], ")"}], "/@", 
      RowBox[{"Tuples", "[", 
       RowBox[{
        RowBox[{"{", 
